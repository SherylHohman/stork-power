{
  "name"        : "workout-tracker",
  "version"     : "1.0.0",
  "description" : "Run statistics on workouts entered. Import and Export your data, so you always have access to it.",

  "main"   : "index.js",
  "scripts"   : {
    "compile" : "src/client/sample-es2015.js -o build/sample-es2015.js",
    "test"    : "mocha && mocha test --require blanket --reporter html-cov > test/coverage.html"
  },

  "repository": {
    "type" : "git",
    "url"  : "git+https://github.com/SherylHohman/stork-power.git"
  },
  "keywords": [
    "workout",
    "statistics",
    "goal",
    "visualizations",
    "thesis",
    "stork-power",
    "workout-tracker",
    "SPA-solo",
    "portfolio",
    "SherylHohman"
  ],
  "author": "Sheryl Hohman",
  "license": "ISC",
  "bugs": {
    "url"   : "https://github.com/SherylHohman/stork-power/issues"
  },
  "homepage": "https://github.com/SherylHohman/stork-power#readme",

  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.17.0",
    "mocha": "^2.5.3",
    "selenium-webdriver": "^2.53.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "sample.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "plugins": [],

  "dependencies": {
    "express": "^4.13.4"
  }
}
